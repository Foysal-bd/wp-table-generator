{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/table-generator",
	"version": "0.1.0",
	"title": "Table Generator",
	"category": "widgets",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"attributes": {
		"headers": {
			"type": "array",
			"default": ["Header"]
		  },
		"tableData": {
			"type": "array",
			"default": [[""]]
		},
		"textAlign": {
			"type": "string",
			"enum": ["left", "center", "right"],
			"default": "left"
		},
		"textColor": {
			"type": "string",
			"default": "#333333"
		},
        "headerTextColor": {
            "type": "string",
            "default": "#222222"
        },
		"backgroundColor": {
			"type": "string",
			"default": "#ffffff"
		},
        "headerBackgroundColor": {
            "type": "string",
            "default": "#f8fafc"
        },
        "striped": {
            "type": "boolean",
            "default": true
        },
        "stripedBackgroundColor": {
            "type": "string",
            "default": "#f9fafb"
        },
		"borderColor": {
			"type": "string",
			"default": "#e5e7eb"
		},
		"borderWidth": {
			"type": "number",
			"default": 1
		},
		"fontSize": {
			"type": "string"
		},
		"fontFamily": {
			"type": "string"
		},
		"fontWeight": {
			"type": "string"
		},
		"fontStyle": {
			"type": "string"
		},
		"textDecoration": {
			"type": "string"
		},
		"textTransform": {
			"type": "string"
		},
		"letterSpacing": {
			"type": "string"
		},
		"lineHeight": {
			"type": "string"
		}
	},
	"supports": {
		"html": false,
		"typography": {
			"fontSize": true,
			"fontFamily": true,
			"fontWeight": true,
			"fontStyle": true,
			"textDecoration": true,
			"textTransform": true,
			"letterSpacing": true,
			"lineHeight": true
		}
	},
	"textdomain": "table-generator",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
